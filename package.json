{
  "name": "nest-boilerplate",
  "version": "1.0.0",
  "description": "A delightful way to building a RESTful API with NodeJs & TypeScript by @nestjs framework",
  "main": "src/main.ts",
  "scripts": {
    "start": "nps",
    "build": "npm start build",
    "presetup": "npm install",
    "idgen": "ts-node ./commands/idgen.ts",
    "test": "npm start test"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nmtoan07/nest-boilerplate.git"
  },
  "keywords": [
    "nest",
    "boilerplate",
    "server"
  ],
  "author": "Toan Nguyen (nmtoan07)",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^4.5.6",
    "@nestjs/core": "^4.5.7",
    "@nestjs/swagger": "^1.1.3",
    "@nestjs/testing": "^4.5.5",
    "@types/body-parser": "^1.16.8",
    "@types/commander": "^2.12.2",
    "@types/couchbase": "^2.1.30",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.11.0",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.6",
    "@types/uuid": "^3.4.3",
    "@types/winston": "^2.3.7",
    "body-parser": "^1.18.2",
    "chalk": "^2.3.0",
    "class-validator": "^0.7.3",
    "commander": "^2.12.2",
    "copyfiles": "^1.2.0",
    "cors": "^2.8.4",
    "couchbase": "^2.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "figlet": "^1.2.0",
    "helmet": "^3.9.0",
    "jsonfile": "^4.0.0",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "nodemon": "^1.14.9",
    "nps": "^5.7.1",
    "nps-utils": "^1.5.0",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.6",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "uuid": "^3.1.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "jest": "^22.0.4",
    "mock-express-response": "^0.2.2",
    "ts-jest": "^22.0.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/test/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "./test/unit/lib/setup.ts"
  }
}
